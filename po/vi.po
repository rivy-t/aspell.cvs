#: prog/aspell.cpp:1878
msgid "Usage: aspell [options] <command>"
msgstr "Cách sử dụng: aspell [tùy_chọn] <lệnh>"

#: prog/aspell.cpp:1879
msgid "<command> is one of:"
msgstr "<lệnh> là một của:"

#: prog/aspell.cpp:1880
msgid "  -?|usage         display a brief usage message"
msgstr "  -?|usage         hiển thị thông điệp ngắn về _cách sử dụng_"

#: prog/aspell.cpp:1881
msgid "  help             display a detailed help message"
msgstr "  help             hiển thị thông điệp _trợ giup_ chi tiết"

#: prog/aspell.cpp:1882
msgid "  -c|check <file>  to check a file"
msgstr "  -c|check <tâp_tin>  _kiểm tra_ tập tin ấy"

#: prog/aspell.cpp:1883
msgid "  -a|pipe          \"ispell -a\" compatibility mode"
msgstr "  -a|pipe          chế độ tương thích \"ispell -a\" (ống)"

#: prog/aspell.cpp:1884
msgid "  [dump] config    dumps the current configuration to stdout"
msgstr "  [dump] config    _gởi toàn_ cấu hình hiện vào thiết bị xuất chuẩn"

#: prog/aspell.cpp:1885
msgid "  config <key>     prints the current value of an option"
msgstr "  config <khóa>     hiển thị trị số hiện của tùy chọn ấy (_cấu hình_)"

#: prog/aspell.cpp:1886
msgid "  [dump] dicts | filters | modes"
msgstr "  [dump] dicts | filters | modes"

#: prog/aspell.cpp:1887
msgid "    lists available dictionaries / filters / filter modes"
msgstr "    hiển thị tất cả từ điển / bộ lọc / chế độ lọc sẵn sàng"

#: prog/aspell.cpp:1888
msgid "[options] is any of the following:"
msgstr "[tùy_chọn] có thể:"

#: prog/aspell.cpp:1901
msgid ""
"  list             produce a list of misspelled words from standard input"
msgstr ""
"  list             tạo _danh sách_ từ chưa có chính tả đúng từ dữ liệu gõ "
"chuẩn"

#: prog/aspell.cpp:1904
msgid ""
"  soundslike       returns the sounds like equivalent for each word entered"
msgstr "  soundslike       gọi điều _nghe như_ mọi từ được nhập"

#: prog/aspell.cpp:1905
msgid "  munch            generate possible root words and affixes"
msgstr "  munch            tạo ra từ gốc và affix (thêm vào) có thể (_nhai_)"

#: prog/aspell.cpp:1906
msgid "  expand [1-4]     expands affix flags"
msgstr "  expand [1-4]     _mở rộng_ cờ affix (thêm vào)"

#: prog/aspell.cpp:1907
msgid ""
"  clean [strict]   cleans a word list so that every line is a valid word"
msgstr ""
"  clean [strict]   sửa đổi danh sách từ để mỗi đường là từ hợp lệ (_dọn "
"sách, chính xác_)"

#: prog/aspell.cpp:1909
msgid "  -v|version       prints a version line"
msgstr "  -v|version       hiển thị đường _phiên bản_"

#: prog/aspell.cpp:1910
msgid "  munch-list [simple] [single|multi] [keep]\n"
msgstr ""
"  munch-list [simple] [single|multi] [keep] (nhai danh sách, đơn giản, đơn, "
"đa, giữ)\n"

#: prog/aspell.cpp:1911
msgid "    reduce the size of a word list via affix compression"
msgstr "    rút gọn cỡ danh sách từ bằng cách nén loại affix (thêm vào)"

#: prog/aspell.cpp:1912
msgid "  conv <from> <to> [<norm-form>]"
msgstr ""
"  conv <from> <to> [<norm-form>] (chuyển đổi, từ, sang, kiểu tiêu chuẩn)"

#: prog/aspell.cpp:1913
msgid "    converts from one encoding to another"
msgstr "    chuyển đổi từ mã sang mã khác"

#: prog/aspell.cpp:1914
msgid "  norm (<norm-map> | <from> <norm-map> <to>) [<norm-form>]"
msgstr "  norm (<norm-map> | <from> <norm-map> <to>) [<norm-form>]"

#: prog/aspell.cpp:1915
msgid "    perform Unicode normalization"
msgstr "    tiêu chuẩn hóa loại Unicode"

#: prog/aspell.cpp:1918
msgid "  dump|create|merge master|personal|repl [<name>]"
msgstr "  dump|create|merge master|personal|repl [<name>]"

#: prog/aspell.cpp:1919
msgid ""
"    dumps, creates or merges a master, personal, or replacement dictionary."
msgstr "    gởi toàn, tạo hay phốí từ điển loại chính, cá nhân hay thay thế"

